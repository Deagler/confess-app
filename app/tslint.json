{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "prettier": true,
    "jsx-no-lambda": false,
    "no-console": false,
    "no-var-requires": false,
    "no-empty": false,
    "triple-equals": false,
    "no-string-literal": false
  }
}
